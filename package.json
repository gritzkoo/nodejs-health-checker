{
  "name": "nodejs-health-checker",
  "version": "1.0.4",
  "description": "Simple Nodejs package to simplify applications based in Node, to trace the healthy of the pods",
  "author": "Gritzko D. Kleiner <gritzkoo@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gritzkoo/nodejs-health-checker.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "healthcheck",
    "health-check",
    "health-checker",
    "healthchecker",
    "pods"
  ],
  "bugs": {
    "url": "https://github.com/gritzkoo/nodejs-health-checker/issues"
  },
  "homepage": "https://github.com/gritzkoo/nodejs-health-checker#readme",
  "scripts": {
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/index.ts\" -e ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "jest --detectOpenHandles --collect-coverage",
    "format": "prettier --config .prettierrc.json --ignore-unknown --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/memcached": "^2.2.6",
    "@types/node": "^14.17.4",
    "@types/node-fetch": "^2.5.10",
    "@types/redis": "^2.8.30",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^1.0.0-rc.5",
    "mariadb": "^2.5.4",
    "memcached": "^2.2.2",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  },
  "files": [
    "dist/**/*"
  ]
}
